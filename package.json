{
  "name": "@sncf/bootstrap-sncf",
  "description": "SNCF frontend framework",
  "version": "4.3.1-r5",
  "sideEffects": false,
  "main": "dist/bootstrap-sncf.min.js",
  "style": "dist/bootstrap-sncf.min.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/SNCFdevelopers/bootstrap-sncf"
  },
  "homepage": "http://sn.cf/socledesign",
  "keywords": [
    "bootstrap",
    "sncf",
    "design",
    "framework",
    "css"
  ],
  "license": "MIT",
  "author": {
    "name": "Fab Design SNCF",
    "email": "design.fab@sncf.fr"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "watch-intern": "webpack --env.documentation --env.theme=intern --watch",
    "watch-extern": "webpack --env.documentation --env.theme=extern --watch",
    "build-intern": "webpack --env.documentation --env.theme=intern",
    "build-intern-dark": "webpack --env.documentation --env.theme=intern --env.darkmode",
    "build-extern": "webpack --env.documentation --env.theme=extern",
    "build-extern-dark": "webpack --env.documentation --env.theme=extern --env.darkmode",
    "dist-intern-full": "webpack --env.theme=intern",
    "dist-intern": "webpack -p --env.production --env.theme=intern",
    "dist-intern-dark": "webpack -p --env.production --env.theme=intern --env.darkmode",
    "dist-extern-full": "webpack --env.theme=extern",
    "dist-extern": "webpack -p --env.production --env.theme=extern",
    "dist-extern-dark": "webpack -p --env.production --env.theme=extern --env.darkmode",
    "docs-compile-intern": "hugo -s . --config config.yml,config.intern.yml",
    "docs-compile-extern": "hugo -s . --config config.yml,config.extern.yml",
    "docs-lint-htmllint": "htmllint --rc build/.htmllintrc \"_gh_pages/**/*.html\" \"src/js/tests/**/*.html\"",
    "docs-serve-intern": "hugo server --port 9001 --disableFastRender --config config.yml,config.intern.yml",
    "docs-serve-extern": "hugo server --port 9001 --disableFastRender --config config.yml,config.extern.yml",
    "svg": "svgo -r src/assets/icons --config=svgo.yml",
    "icofont": "icon-font-generator src/assets/icons/*.svg -o src/assets/fonts/icons --csspath src/scss/common/generic/_icons.css --prefix icons --fontspath ../assets/fonts/icons/ --html false -j false"
  },
  "devDependencies": {
    "ajv": "^6.7.0",
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^4.5.1",
    "cpx": "^1.5.0",
    "css-loader": "^2.1.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-compat": "^2.2.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^1.1.11",
    "htmllint-cli": "0.0.6",
    "icon-font-generator": "^1.1.2",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.1.3",
    "precss": "^3.1.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.0.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "svgo": "^1.3.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.1"
  },
  "dependencies": {
    "bootstrap": "4.3.1",
    "chart.js": "^2.7.2",
    "chartjs-plugin-zoom": "^0.6.3",
    "flatpickr": "^4.6.2",
    "hugo-bin": "^0.44.1",
    "ion-rangeslider": "^2.2.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "null-loader": "^1.0.0",
    "popper.js": "^1.14.1",
    "swiper": "^4.2.2",
    "whatwg-fetch": "^3.0.0",
    "yarn": "^1.7.0"
  }
}
