{
  "name": "sncf",
  "description": "SNCF frontend framework",
  "version": "1.0.0",
  "sideEffects": false,
  "main": "index.js",
  "scripts": {
    "watch-intern": "webpack --env.theme=intern --watch",
    "watch-extern": "webpack --env.theme=extern --watch",
    "build-intern": "webpack --env.theme=intern",
    "build-extern": "webpack --env.theme=extern",
    "dist-intern": "webpack -p --env.production --env.theme=intern",
    "dist-extern": "webpack -p --env.production --env.theme=extern",
    "docs-compile-intern": "bundle exec jekyll build --config _config.intern.yml",
    "docs-compile-extern": "bundle exec jekyll build --config _config.extern.yml",
    "docs-lint-htmllint": "htmllint --rc build/.htmllintrc \"_gh_pages/**/*.html\" \"src/js/tests/**/*.html\"",
    "docs-serve-intern": "bundle exec jekyll serve --config _config.intern.yml",
    "docs-serve-extern": "bundle exec jekyll serve --config _config.extern.yml",
    "icofont": "icon-font-generator src/assets/icons/*.svg -o src/assets/fonts/icons --csspath src/scss/common/generic/_icons.css --fontspath ../assets/fonts/icons/ --html false -j false"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.5.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-compat": "^2.2.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^1.1.11",
    "htmllint-cli": "0.0.6",
    "icon-font-generator": "^1.1.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.1.3",
    "precss": "^3.1.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.0.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13"
  },
  "dependencies": {
    "bootstrap": "4.0.0",
    "chart.js": "^2.7.2",
    "chartjs-plugin-zoom": "^0.6.3",
    "flatpickr": "https://github.com/vdesdoigts/flatpickr.git#dist",
    "ion-rangeslider": "^2.2.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.5",
    "popper.js": "^1.14.1",
    "swiper": "^4.2.2",
    "yarn": "^1.7.0"
  }
}
